<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iglesianavarra">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_index"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_index_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IglesiaNavarra">
        <activity android:name="apps.icm.iglesianavarra.MainActivity"></activity>
        <activity android:name="apps.icm.iglesianavarra.SegundaActivity2" />
        <activity android:name="apps.icm.iglesianavarra.LibrosActivity2" />
        <activity android:name="apps.icm.iglesianavarra.RegistrarseActivity" />
        <activity android:name="apps.icm.iglesianavarra.IniciarSesionActivity" />
        <activity android:name="apps.icm.iglesianavarra.splash_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>